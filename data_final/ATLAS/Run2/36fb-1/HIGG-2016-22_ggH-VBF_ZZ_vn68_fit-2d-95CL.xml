<!--
parametrization using Variable Gaussian distribution from 2D grids
for mu(VBF, ZZ) and mu(ggH, ZZ) from Fig. 8a, 95% CL
at mH = 125.09 GeV, shown in HIGG-2016-22 
http://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2016-22/
-->

<expmu decay="ZZ" dim="2" type="vn">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2016-22</source>
  <sqrts>13</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="VBF">1.0</eff>
  <eff axis="y" prod="ggH">1.0</eff>

  <bestfit>
    <x>3.917193333</x>
    <y>1.117641159</y>
  </bestfit>
 
  <param>
    <uncertainty axis="x" side="left">-1.43621151979</uncertainty>
    <uncertainty axis="x" side="right">+1.72470628359</uncertainty>
    <uncertainty axis="y" side="left">-0.21316543549</uncertainty>
    <uncertainty axis="y" side="right">+0.23759738123</uncertainty>
    <correlation>-0.383631667943</correlation>
  </param>
</expmu>
