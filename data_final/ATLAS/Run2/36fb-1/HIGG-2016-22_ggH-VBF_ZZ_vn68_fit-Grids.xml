<!--
parametrization using Variable Gaussian distribution from 2D grids
for mu(VBF, ZZ) and mu(ggH, ZZ) from Fig. 8a, 95%
at mH = 125.09 GeV, shown in HIGG-2016-22 
http://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2016-22/
-->

<expmu decay="ZZ" dim="2" type="vn">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2016-22</source>
  <sqrts>13</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="VBF">1.0</eff>
  <eff axis="y" prod="ggH">1.0</eff>

  <bestfit>
    <x>3.8587855175</x>
    <y>1.11435560099</y>
  </bestfit>
 
  <param>
    <uncertainty axis="x" side="left">-1.74865865</uncertainty>
    <uncertainty axis="x" side="right">+2.05910747</uncertainty>
    <uncertainty axis="y" side="left">-0.2461435</uncertainty>
    <uncertainty axis="y" side="right">+0.27109778</uncertainty>
    <correlation>-0.41</correlation>
  </param>
</expmu>
