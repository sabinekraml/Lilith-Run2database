<!--
ATLAS result for mu(ggH, ZZ) & mu(VBF, ZZ) at mH = 125.09 GeV,  
with profile likelihood fitted from Aux. Fig. 7a & 7b
and their correlation taken from Aux. Fig. 4c, shown in HIGG-2016-22
http://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2016-22/
-->

<expmu decay="ZZ" dim="2" type="vn">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2016-22</source>
  <sqrts>13</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="ggH">1.0</eff>
  <eff axis="y" prod="VBF">1.0</eff>

  <bestfit>
    <x>1.11436</x>
    <y>3.85879</y>
  </bestfit>
 
  <param>
    <uncertainty axis="x" side="left">-0.22119</uncertainty>
    <uncertainty axis="x" side="right">+0.24614</uncertainty>
    <uncertainty axis="y" side="left">-1.43821</uncertainty>
    <uncertainty axis="y" side="right">+1.74866</uncertainty>
    <correlation>-0.41</correlation>
  </param>
</expmu>
