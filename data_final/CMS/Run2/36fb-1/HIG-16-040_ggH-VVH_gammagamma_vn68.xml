<!--
parametrization of a 2D Gaussian fitted from the 95% CL contour
in the plane mu(gg+ttH, gamma gamma) vs. mu(VBF+VH, gamma gamma)
with profiled Higgs mass 125 GeV, shown in CMS-HIG-16-040 Fig. 19 
http://cms-results.web.cern.ch/cms-results/public-results/publications/HIG-16-040/
-->

<expmu decay="gammagamma" dim="2" type="vn">
  <experiment>CMS</experiment>
  <source type="published">HIG-16-040</source>
  <sqrts>13</sqrts>
  <mass>125</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="ggH">1.0</eff>
  <eff axis="y" prod="VVH">1.0</eff>

  <bestfit>
    <x>1.10</x>
    <y>1.187975172</y>
  </bestfit>
 
  <param>
    <uncertainty axis="x" side="left">-0.186271746694</uncertainty>
    <uncertainty axis="x" side="right">+0.216595804003</uncertainty>
    <uncertainty axis="y" side="left">-0.506546964828</uncertainty>
    <uncertainty axis="y" side="right">+0.584575902055</uncertainty>
    <correlation>-0.367768622055</correlation>
  </param>
</expmu>
