<!--
ATLAS result for mu(ttH, tautau) at mH = 125 GeV
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2017-02/
 -->

<expmu decay="tautau" dim="1" type="n">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2017-02</source>
  <sqrts>13</sqrts>
  <mass>125</mass>

  <eff prod="ttH">1</eff>

  <bestfit>1.5</bestfit>
  
  <param>
    <uncertainty side="left">-1.0</uncertainty>
    <uncertainty side="right">1.2</uncertainty>
    <!-- if not needed, just <uncertainty></uncertainty> -->
  </param>
</expmu>
