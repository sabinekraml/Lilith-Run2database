<!--
parametrization using Variable Gaussian distribution from 2D grids
for mu(ggH, gammagamma) & mu(VBF, gammagamma) from Fig. 15, 95% CL
at mH = 125.09 GeV, shown in HIGG-2016-21
http://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2016-21/
-->

<expmu decay="gammagamma" dim="2" type="vn">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2016-21</source>
  <sqrts>13</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="ggH">1.0</eff>
  <eff axis="y" prod="VBF">1.0</eff>

  <bestfit>
    <x>0.812281569</x>
    <y>2.055330645</y>
  </bestfit>
 
  <param>
    <uncertainty axis="x" side="left">-0.177361357316</uncertainty>
    <uncertainty axis="x" side="right">+0.185154007923</uncertainty>
    <uncertainty axis="y" side="left">-0.55544614525</uncertainty>
    <uncertainty axis="y" side="right">+0.664606029766</uncertainty>
    <correlation>-0.24364227856</correlation>
  </param>
</expmu>
