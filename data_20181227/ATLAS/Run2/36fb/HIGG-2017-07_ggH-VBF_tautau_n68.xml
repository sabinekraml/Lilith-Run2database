<!--
parametrization of a 2D Gaussian fitted from the 68% CL contour
in the plane mu(ggH, tau tau) vs. mu(VBF, tau tau)
at mH = 125 GeV, shown in Aux. Fig. 5 in HIGG-2017-07
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2017-07/
-->

<expmu decay="tautau" dim="2" type="n">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2017-07</source>
  <sqrts>13</sqrts>
  <mass>125</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="ggH">1.0</eff>
  <eff axis="y" prod="VBF">1.0</eff>

  <bestfit>
    <x>1.11239096</x>
    <y>1.21142269</y>
  </bestfit>
  
  <param>
    <a>3.2993942</a>
    <b>1.50299369</b>
    <c>3.77001839</c>
  </param>
</expmu>
