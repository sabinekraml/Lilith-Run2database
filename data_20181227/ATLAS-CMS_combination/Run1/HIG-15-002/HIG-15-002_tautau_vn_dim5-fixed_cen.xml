<!--
CMS result for mu(combined H production modes, tautau) at mH = 125.09 GeV
with profile likelihood taken from Table 8  
and their correlations taken from Fig. 27, shown in HIG-15-002
http://cms-results.web.cern.ch/cms-results/public-results/publications/HIG-15-002/
-->

<expmu dim="5" type="vn">
  <experiment>CMS</experiment>
  <source type="published">HIG-15-002</source>
  <sqrts>7+8</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="d1" prod="ggH" decay="tautau">1.0</eff>
  <eff axis="d2" prod="VBF" decay="tautau">1.0</eff> 
  <eff axis="d3" prod="WH" decay="tautau">1.0</eff>  
  <eff axis="d4" prod="ZH" decay="tautau">1.0</eff>
  <eff axis="d5" prod="ttH" decay="tautau">1.0</eff>

  <bestfit>
    <d1>1.074380165</d1>
	<d2>1.25</d2>
	<d3>-1.44469526</d3>	
	<d4>2.222222222</d4>
	<d5>-1.851851852</d5>
  </bestfit>
 
  <param>
    <uncertainty axis="d1" side="left">-0.6</uncertainty>
    <uncertainty axis="d1" side="right">+0.6</uncertainty>
    <uncertainty axis="d2" side="left">-0.4</uncertainty>
    <uncertainty axis="d2" side="right">+0.4</uncertainty>
    <uncertainty axis="d3" side="left">-1.4</uncertainty>
    <uncertainty axis="d3" side="right">+1.4</uncertainty>
    <uncertainty axis="d4" side="left">-1.8</uncertainty>
    <uncertainty axis="d4" side="right">+2.2</uncertainty>
    <uncertainty axis="d5" side="left">-3.3</uncertainty>
    <uncertainty axis="d5" side="right">+3.7</uncertainty>
	
    
	<correlation entry="d1d2">-0.37</correlation>
	<correlation entry="d1d3">-0.25</correlation>
	<correlation entry="d1d4">-0.21</correlation>
	<correlation entry="d1d5">0</correlation>
   
	<correlation entry="d2d3">0.05</correlation>
	<correlation entry="d2d4">0.04</correlation>
	<correlation entry="d2d5">0</correlation>
   
	<correlation entry="d3d4">-0.02</correlation>
	<correlation entry="d3d5">0</correlation>
   
	<correlation entry="d4d5">0</correlation>
  </param>
</expmu>
