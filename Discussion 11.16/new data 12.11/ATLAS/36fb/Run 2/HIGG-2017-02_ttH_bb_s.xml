<!--
ATLAS result for mu(ttH, bb) at mH = 125 GeV
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/PAPERS/HIGG-2017-02/
 -->

<expmu decay="bb" dim="1" type="n">
  <experiment>ATLAS</experiment>
  <source type="published">HIGG-2017-02</source>
  <sqrts>13</sqrts>
  <mass>125</mass>

  <eff prod="ttH">1</eff>

  <bestfit>0.8</bestfit>
  
  <param>
    <uncertainty side="left">-0.6</uncertainty>
    <uncertainty side="right">0.6</uncertainty>
    <!-- if not needed, just <uncertainty></uncertainty> -->
  </param>
</expmu>
