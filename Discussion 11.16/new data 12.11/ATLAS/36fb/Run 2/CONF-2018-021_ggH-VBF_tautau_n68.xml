<!--
parametrization of a 2D Gaussian fitted from the 68% CL contour
in the plane mu(ggH, tau tau) vs. mu(VBF, tau tau)
at mH = 125 GeV, shown in ATLAS-CONF-2018-021
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/CONFNOTES/ATLAS-CONF-2018-021/
-->

<expmu decay="WW" dim="2" type="n">
  <experiment>ATLAS</experiment>
  <source type="preliminary">CONF-2018-021</source>
  <sqrts>13</sqrts>
  <mass>125</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="ggH">1.0</eff>
  <eff axis="y" prod="VBF">1.0</eff>

  <bestfit>
    <x>1.06072087</x>
    <y>1.22541677</y>
  </bestfit>
  
  <param>
    <a>3.51097895</a>
    <b>1.5768233</b>
    <c>3.8294795</c>
  </param>
</expmu>
