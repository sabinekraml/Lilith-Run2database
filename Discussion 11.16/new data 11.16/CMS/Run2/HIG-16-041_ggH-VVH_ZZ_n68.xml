<!--
parametrization of a 2D Gaussian fitted from the 68% CL contour
in the plane mu(gg+ttH, ZZ) vs. mu(VBF+VH, ZZ)
at mH = 125.09 GeV, shown in CMS-HIG-16-041
http://cms-results.web.cern.ch/cms-results/public-results/publications/HIG-16-041/index.html
-->

<expmu decay="ZZ" dim="2" type="n">
  <experiment>CMS</experiment>
  <source type="published">HIG-16-041</source>
  <sqrts>13</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="ggH">1.0</eff>
  <eff axis="y" prod="VVH">1.0</eff>

  <bestfit>
    <x>1.20</x>
    <y>0.00</y>
  </bestfit>
  
  <param> 
    <a>26.3</a>
    <b>2.53</b>
    <c>1.47</c>
  </param>
</expmu>
